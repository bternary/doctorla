            <li class="@ViewBag.Anasayfa"><a href="/NursePanel/Anasayfa"><i class="sl sl-icon-settings"></i> Anasayfa</a></li>
            <li class="@ViewBag.Profilim"><a href="/NursePanel/Profilim"><i class="sl sl-icon-user"></i> Profilim</a></li>

            <li class="@ViewBag.GecmisRandevular"><a href="/NursePanel/GecmisPaketler"><i class="sl sl-icon-list"></i>Paketler</a></li>
